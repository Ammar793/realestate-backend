# Multi-Agent System Configuration
# Copy this file to .env and fill in your actual values

# AWS Configuration
AWS_REGION=us-west-2
KNOWLEDGE_BASE_ID=your-knowledge-base-id
MODEL_ARN=arn:aws:bedrock:us-west-2::foundation-model/anthropic.claude-3-5-sonnet-20241022-v2:0

# AgentCore Gateway Configuration
GATEWAY_NAME=JLSRealEstateGateway
LAMBDA_ARN=arn:aws:lambda:us-west-2:123456789012:function:your-lambda-function-name

# Cognito Authentication Configuration
COGNITO_CLIENT_ID=your-cognito-client-id
COGNITO_CLIENT_SECRET=your-cognito-client-secret
COGNITO_TOKEN_URL=https://your-domain.auth.us-west-2.amazoncognito.com/oauth2/token
COGNITO_USER_POOL_ID=your-user-pool-id
COGNITO_IDENTITY_POOL_ID=your-identity-pool-id

# Optional: Direct Gateway Configuration (if not using setup script)
# AGENTCORE_GATEWAY_URL=https://your-gateway-url.amazonaws.com
# AGENTCORE_ACCESS_TOKEN=your-access-token

# Gateway Server Configuration (for local development)
GATEWAY_HOST=0.0.0.0
GATEWAY_PORT=8000

# Agent Configuration
AGENT_LOG_LEVEL=INFO
AGENT_TIMEOUT=30
AGENT_MAX_RETRIES=3
AGENT_MAX_TOOL_INVOCATIONS=2

# Workflow Configuration
WORKFLOW_PARALLEL_LIMIT=5
WORKFLOW_TIMEOUT=120

# API Configuration
API_RATE_LIMIT=100
API_TIMEOUT=30

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_DESTINATION=cloudwatch 